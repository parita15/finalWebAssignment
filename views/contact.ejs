<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Contact Us</title>
    </head>
    <%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>
    <body>
        <form style="margin-left: 80px;" action="/send" method="post">
            <div class="form-group  col-2" >
                <label>First Name: </label>
                <input class="form-control" type="text" id="fname" name="fname"/>
            </div>
            <div style="margin-top: -10px;" class="form-group  col-2">
                <label>Last Name: </label>
                <input class="form-control" type="text" id="lname" name="lname"/>
            </div>
            <div style="margin-top: -10px;" class="form-group  col-2">
                <label>Contact Number: </label>
                <input class="form-control" type="text" id="cnumber" name="cnumber"/>
            </div>
            <div style="margin-top: -10px;" class="form-group  col-2">
                <label>Email Address: </label>
                <input class="form-control" type="email" id="emailadd" name="emailadd"/>
            </div>
            <div style="margin-top: -10px;" class="form-group  col-2">
                <label>Message: </label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="2"></textarea>
            </div>
            <div style="margin-left: 20px; margin-top: -10px">
                <input type="button" name="newID" placeholder="Button">
                <button class="btn btn-warning">send</button>
                <!-- <button type="submit" class="btn btn-warning"><a href="/home" class="nav-link px-2" style="color: black; font-weight:bold;padding: 1px;">Submit</a></button> -->
            </div>
        </form>
    </body>
    <%- include partials/bottom_nav.ejs %>  
    <%- include partials/footer.ejs %>
</html>